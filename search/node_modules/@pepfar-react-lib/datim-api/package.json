  "license": "ISC",
  "private": false,
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@pepfar-react-lib/dep-copy": "^0.0.9",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "node-fetch": "^3.2.4",
    "ts-node": "^10.7.0"
  },
  "type": "module",
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!node-fetch)/"
    ],
    "watchPathIgnorePatterns": [
