{
  "name": "@pepfar-react-lib/testwrap",
  "version": "0.0.9",
  "description": "Intuitive wrapper for @testing-library",
  "main": "index.ts",
  "exports": {
    ".": {
      "import": "./index.ts"
    },
    "jsbuild": {
      "import": "./jsbuild/index.js"
    }
  },
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest",
    "build:js": "rm -rf jsbuild/* && tsc --outDir jsbuild"
  },
  "peerDependencies": {
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.43",
    "jest": "^27.5.1",
    "react": "^17.0.2"
  },
  "author": "Jakub Flaska",
  "license": "ISC",
  "private": false,
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7"
  }
}
